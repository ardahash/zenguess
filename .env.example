# ZenMarkets Environment Variables
# Copy this file to .env.local and fill in the values

# Default chain: "testnet" or "mainnet"
NEXT_PUBLIC_DEFAULT_CHAIN=testnet

# Horizen RPC endpoints (leave blank for defaults)
NEXT_PUBLIC_HORIZEN_RPC_HTTP=
NEXT_PUBLIC_HORIZEN_RPC_WS=

# Explorer and Bridge URLs (leave blank for defaults)
NEXT_PUBLIC_EXPLORER_URL=
NEXT_PUBLIC_BRIDGE_URL=

# Goldsky subgraph endpoint (for production indexing)
NEXT_PUBLIC_GOLDSKY_ENDPOINT=
